{
  "expo": {
    "name": "aimfit-mobile",
    "slug": "aimfit-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/aimfit-logo.png",
    "splash": {
      "image": "./dumbbell.jpeg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "jesse.aimfit",
      "buildNumber": "1.0.0"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
